pipeline {
    agent any

    stages {
        stage('Morning') {
            steps {
                echo 'Good Morning ! '
            }
        }
stage('Noon') {
            steps {
                echo 'Good Afternoon  ! '
            }
        }
stage('Eve') {
            steps {
                echo 'Good Evening! '
            }
        }
stage('Night') {
            steps {
                echo 'Good Night ! '
            }
        }
    }
}
